{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "react/jsx-filename-extension": 0,
      "import/prefer-default-export": 0,
      "react-hooks/rules-of-hooks": 0,
      "react-hooks/exhaustive-deps": 0,
      "import/no-extraneous-dependencies":0,
      "default-param-last":0,
      "func-names":0,
      "no-console":0
    }
}
